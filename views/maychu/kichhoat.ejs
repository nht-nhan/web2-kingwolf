    <!--end:: Global Optional Vendors -->

    <!--begin::Global Theme Styles(used by all pages) -->

    <link href="kw/demo/default/base/style.bundle.css" rel="stylesheet" type="text/css" />
    <!--end::Global Theme Styles -->
    <!--begin::Layout Skins(used by all pages) -->
    <link href="kw/demo/default/skins/header/base/light.css" rel="stylesheet" type="text/css" />
    <link href="kw/demo/default/skins/header/menu/light.css" rel="stylesheet" type="text/css" />
    <link href="kw/demo/default/skins/brand/navy.css" rel="stylesheet" type="text/css" />
    <link href="kw/demo/default/skins/aside/navy.css" rel="stylesheet" type="text/css" />
    <!--end::Layout Skins -->
    <link rel="shortcut icon" href="kw/media/logos/favicon.ico" />
</head>
