<!-- begin::Body -->

<body class="kt-login-v2--enabled kt-header--fixed kt-header-mobile--fixed kt-subheader--enabled kt-subheader--transparent kt-aside--enabled kt-aside--fixed kt-page--loading">
    <!-- begin:: Page -->
    <div class="kt-grid kt-grid--ver kt-grid--root">
        <div class="kt-grid__item kt-grid__item--fluid kt-grid kt-grid kt-grid--hor kt-login-v2" id="kt_login_v2">
            <!--begin::Item-->
            <div class="kt-grid__item kt-grid--hor">
                <!--begin::Heade-->
                <div class="kt-login-v2__head">
                    <div class="kt-login-v2__logo">
                        <a href="#">
                            <img src="kw/media/logos/logo-5.png" alt="" />
                        </a>
                    </div>
                    <div class="kt-login-v2__signup"> <span>Bạn đã nhớ ra tài khoản?</span> <a href="/dangnhap" class="kt-link kt-font-brand">Đăng Nhập</a> </div>
                </div>
                <!--begin::Head-->
            </div>
            <!--end::Item-->
            <!--begin::Item-->
            <div class="kt-grid__item kt-grid kt-grid--ver kt-grid__item--fluid">
                <!--begin::Body-->
                <div class="kt-login-v2__body">
                    <!--begin::Wrapper-->
                    <div class="kt-login-v2__wrapper">
                        <div class="kt-login-v2__container">
                            <div class="kt-login-v2__title">
                                <%
                                        if(type==1)
                                        {
                                            %>
                                    <div class="modal fade show" id="exampleModalCenter" tabindex="-1" role="dialog" aria-labelledby="exampleModalCenterTitle" aria-modal="true" style="display: block; padding-right: 17px;">
                                        <div class="modal-dialog modal-dialog-centered" role="document">
                                            <div class="modal-content">
                                                <div class="modal-header">
                                                    <h5 class="modal-title" id="exampleModalCenterTitle">
                                                        Kích Hoạt Thành Công
                                                    </h5>
                                                </div>
                                                <div class="modal-body">
                                                    <p>Vui lòng kiểm tra lại email lấy <strong>mật khẩu</strong> để đăng nhập</p>
                                                    <p>Bạn bấm <strong>Chuyển trang</strong> để vào trang đăng nhập</p>
                                                </div>
                                                <div class="modal-footer">
                                                    <a class="btn btn-primary" href="/dangnhap">Chuyển trang</a>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <%
                                        }
                                        else
                                        {
                                            %>
                                        <div class="modal fade show" id="exampleModalCenter" tabindex="-1" role="dialog" aria-labelledby="exampleModalCenterTitle" aria-modal="true" style="display: block; padding-right: 17px;">
                                            <div class="modal-dialog modal-dialog-centered" role="document">
                                                <div class="modal-content">
                                                    <div class="modal-header">
                                                        <h5 class="modal-title" id="exampleModalCenterTitle">
                                                            Kích Hoạt Không Thành Công
                                                        </h5>
                                                    </div>
                                                    <div class="modal-body">
                                                        <p>Token của bạn hết hạn hoặc token bị sai !</p>
                                                        <p>Vui lòng bạn kiểm tra lại !(email)</p>
                                                        <p>Bạn bấm <strong>Chuyển trang</strong> để vào trang chủ</p>
                                                    </div>
                                                    <div class="modal-footer">
                                                        <a class="btn btn-primary" href="/">Chuyển trang</a>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <%
                                        }
                                    %>
                            </div>
                        </div>
                    </div>
                    <!--end::Wrapper-->

                </div>
                <!--begin::Body-->
            </div>
            <!--end::Item-->
            <!--begin::Item-->
            <div class="kt-grid__item">
                <div class="kt-login-v2__footer">
                    <div class="kt-login-v2__info"> <a href="#" class="kt-link">&copy; 2020 KingWolf Bank</a> </div>
                </div>
            </div>
            <!--end::Item-->
        </div>
    </div>
    <!-- end:: Page -->
