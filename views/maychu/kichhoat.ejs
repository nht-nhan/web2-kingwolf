    <link href="kw/vendors/general/sweetalert2/dist/sweetalert2.css" rel="stylesheet" type="text/css" />
    <link href="kw/vendors/general/socicon/css/socicon.css" rel="stylesheet" type="text/css" />
    <link href="kw/vendors/custom/vendors/line-awesome/css/line-awesome.css" rel="stylesheet" type="text/css" />
    <link href="kw/vendors/custom/vendors/flaticon/flaticon.css" rel="stylesheet" type="text/css" />
    <link href="kw/vendors/custom/vendors/flaticon2/flaticon.css" rel="stylesheet" type="text/css" />
